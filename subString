int subString (char *array1, char *array2, int startIndex, int endIndex)
		{
				
			int position;

		if (array1 == NULL)
				{
					return 1;
				}

			for (position = startIndex; position <= endIndex; position++)
        {
            printf("%c", array1[position]);
						*array2 = array1[position];


        }
         printf("\n");
					return 0;
		}
